(define-macro
  (if arg body ...)
  (cond (arg body ...)))

(define-macro
  (function body ...)
  (lambda body ...))

(define-macro
  (let ((key value) ...)
    body ...)
 ((lambda (key ...)
   body ...) value ...))

(define-macro
  (__loop_include__ filename)
  (__loop_eval__
    (let ((buf (fs.readFileSync filename)))
      ; eek! shouldn't need this return
      (return (loop.compile (buf.toString))))))
