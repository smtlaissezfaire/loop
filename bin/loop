#!/usr/bin/env node

var loop = require(__dirname + '/../lib/loop');

loop.executable.run({
  argv: process.argv,
  compilationOptions: {
    indent_start : 0,
    indent_level : 2,
    quote_keys   : false,
    space_colon  : true,
    beautify     : true,
    ascii_only   : false,
    inline_script: false
  },
  module: loop.compile
});
